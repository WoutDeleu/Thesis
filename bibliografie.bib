@mastersthesis{Verdonck_2022,
  address      = {Leuven},
  title        = {Inferentie-aanvallen met hoogteprofielen tegen (endpoint) privacy zones in fitness tracking sociale netwerken},
  abstractnote = {Fitness tracking sociale netwerken geven gebruikers de mogelijkheid hun sportactiviteiten op te nemen en te delen met andere gebruikers. Deze sociale netwerken zijn handig in het bijhouden van eigen prestaties en het vergelijken met andere gebruikers, maar brengen een privacyrisico met zich mee. Malafide gebruikers kunnen uit locatiegegevens van de activiteiten de woon- of werkplaats van andere gebruikers afleiden. Endpoint privacy zones (EPZs) zijn ontwikkeld om dit te verhinderen. Dit zijn veilige zones rond een beschermde locatie waarin het traject van een activiteit niet weergegeven wordt.  Eerder onderzoek wees al uit dat EPZs kwetsbaar zijn voor inferentie-aanvallen waarbij de afgelegde afstand gebruikt wordt. Activiteiten bevatten echter nog informatie, zoals een hoogteprofiel, die mogelijks gebruikt kan worden voor een inferentie-aanval. Dit werk toont aan dat EPZs ook kwetsbaar zijn voor inferentie-aanvallen die gebruikmaken van hoogteprofielen. Dit wordt gedaan door aanvallen uit te voeren op gebruikers van het populaire fitness tracking sociale netwerk Strava. De aanval combineert de totale hoogtetoename en de informatie over het hoogteprofiel die na het toepassen van een EPZ nog aanwezig is met het wegennetwerk en de toegangspunten tot de EPZ. Met die informatie kan een aanvaller de mogelijke paden in de EPZ bepalen en met een regressieanalyse de beschermde locatie voorspellen.  Eerst wordt een poging gedaan om de berekening van de hoogtetoename van Strava te reverse engineeren. Deze berekening kan benaderd worden door verschillende methodes te implementeren en te vergelijken met de berekeningsmethode van Strava. De methode die Strava het best benadert, heeft een gemiddelde afwijking van 6,50 meter. Vervolgens wordt de aanval geïmplementeerd en getest op een dataset van 100 echte gebruikers van Strava. De aanval haalt een success rate tot 36,36% op de geteste EPZs. Vooral afwijkingen bij het berekenen van de hoogtetoename en grotere radii zorgen voor een lage success rate. De success rate stijgt wanneer er veel en geografisch gespreide activiteiten zijn. Gebieden met een dicht wegennet en een vlak reliëf verminderen de kans op een succesvolle aanval.},
  note         = {Book Title: Inferentie-aanvallen met hoogteprofielen tegen (endpoint) privacy zones in fitness tracking sociale netwerken},
  school       = {KU Leuven. Faculteit Industriële Ingenieurswetenschappen},
  author       = {Verdonck, Tibo},
  year         = {2022},
  language     = {dut}
} 
@article{Carr_Hayes_2015,
  title        = {Social Media: Defining, Developing, and Divining},
  volume       = {23},
  issn         = {1545-6870},
  doi          = {10.1080/15456870.2015.972282},
  abstractnote = {What is a social medium, and how may one moderate, isolate, and influence communicative processes within? Although scholars assume an inherent understanding of social media based on extant technology, there is no commonly accepted definition of what social media are, both functionally and theoretically, within communication studies. Given this lack of understanding, cogent theorizing regarding the uses and effects of social media has been limited. This work first draws on extant definitions of social media and subcategories (e.g., social network sites) from public relations, information technology, and management scholarship, as well as the popular press, to develop a definition of social media precise enough to embody these technologies yet robust enough to remain applicable in 2035. It then broadly explores emerging developments in the features, uses, and users of social media for which future theories will need to account. Finally, it divines and prioritizes challenges that may not yet be apparent to theorizing communication processes with and in mercurial social media. We address how social media may uniquely isolate and test communicative principles to advance our understanding of human–human and human–computer interaction. In all, this article provides a common framework to ground and facilitate future communication scholarship and beyond.},
  number       = {1},
  journal      = {Atlantic Journal of Communication},
  publisher    = {Routledge},
  author       = {Carr, Caleb T. and Hayes, Rebecca A.},
  year         = {2015},
  month        = {Jan},
  pages        = {46–65}
} 
